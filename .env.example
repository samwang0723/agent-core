# LLM model api keys
ANTHROPIC_API_KEY=your-api-key
OPENAI_API_KEY=your-api-key
GOOGLE_API_KEY=your-api-key

# Visual Crossing Weather API Key
VISUAL_CROSSING_API_KEY=your_visual_crossing_api_key_here

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/api/v1/auth/google/callback

# Database
DATABASE_URL=postgresql://postgres:postgres@0.0.0.0:5432/agent-core-main

# Mastra Memory
MASTRA_MEMORY_MODULE=libsql
MASTRA_USING_VNEXT_NETWORK=true

# Tools
PORTFOLIO_URL=

# Timeout Configuration
MCP_INIT_TIMEOUT=5000
MCP_TIMEOUT=30000
MODEL_TIMEOUT_MS=8000
WEATHER_API_TIMEOUT=5000
PORTFOLIO_API_TIMEOUT=3000

# Performance Monitoring
ENABLE_PERFORMANCE_LOGGING=true
PERFORMANCE_LOG_LEVEL=info

NODE_ENV=production
LOG_LEVEL=info
PORT=3000
